#%Module1.0

module load hpcenc
conflict wine-8.0.1
set __encroot $::env(ENCROOT)
set _MROOT ${__encroot}/deps/wine64-8.0.1
set _LPATH ${_MROOT}/lib
prepend-path LIBRARY_PATH ${_LPATH}
prepend-path LD_LIBRARY_PATH ${_LPATH}
prepend-path CPATH ${_MROOT}/include
prepend-path PATH ${_MROOT}/bin
prepend-path MANPATH ${_MROOT}/share/man
setenv WINEPREFIX ${__encroot}/deps/wine/wine64-8.0.1
setenv WINEFONTDIR ${__encroot}/deps/wine/wine64-8.0.1/drive_c/windows/Fonts
setenv WINEAVSDIR ${__encroot}/deps/wine/avs64
setenv WINEAVSPLGNDIR ${__encroot}/deps/wine/avs64/plugins
