#%Module1.0

module load hpcenc centos7-i686
conflict wine64-8.0.1
set __encroot $::env(ENCROOT)
set _MROOT ${__encroot}/deps/wine-8.0.1
set _LPATH ${_MROOT}/lib
prepend-path LIBRARY_PATH ${_LPATH}
prepend-path LD_LIBRARY_PATH ${_LPATH}
prepend-path CPATH ${_MROOT}/include
prepend-path PATH ${_MROOT}/bin
prepend-path MANPATH ${_MROOT}/share/man
setenv WINEPREFIX ${__encroot}/deps/wine/wine-8.0.1
setenv WINEFONTDIR ${__encroot}/deps/wine/wine-8.0.1/drive_c/windows/Fonts
setenv WINEAVSDIR ${__encroot}/deps/wine/avs32
setenv WINEAVSPLGNDIR ${__encroot}/deps/wine/avs32/plugins
setenv RTLD /home/src/HPCENC/hpcenc/deps/centos7-i686/lib/ld-2.17.so
setenv WINE_PRELOADER /home/src/HPCENC/hpcenc/deps/centos7-i686/lib/ld-2.17.so
